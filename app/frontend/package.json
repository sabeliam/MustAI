{
    "name": "must-ai",
    "version": "0.0.4",
    "scripts": {
        "ng": "ng",
        "start": "npm run config -- --environment=prod && ng serve",
        "build:ci": "npm run config -- --environment=prod && npm run build:prod",
        "build:prod": "ng build --configuration production",
        "build:stats": "npm run build --stats-json",
        "analyze": "webpack-bundle-analyzer dist/MustAI/stats.json",
        "config": "ts-node src/scripts/set-env.ts",
        "watch": "npm run config -- --environment=dev && ng build --watch --configuration development",
        "test": "ng test",
        "test:headless": "ng test --watch=false --browsers=ChromeHeadless",
        "mock-server": "npx json-server --watch db.json"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~15.2.0",
        "@angular/cdk": "^15.2.3",
        "@angular/common": "~15.2.0",
        "@angular/compiler": "~15.2.0",
        "@angular/core": "~15.2.0",
        "@angular/forms": "~15.2.0",
        "@angular/platform-browser": "~15.2.0",
        "@angular/platform-browser-dynamic": "~15.2.0",
        "@angular/router": "~15.2.0",
        "@angular/service-worker": "~15.2.0",
        "@dalenguyen/openai": "^0.11.3",
        "@ngxs/store": "^3.7.6",
        "@taiga-ui/addon-mobile": "^3.19.0",
        "@taiga-ui/cdk": "^3.19.0",
        "@taiga-ui/core": "^3.19.0",
        "@taiga-ui/icons": "^3.19.0",
        "@taiga-ui/kit": "^3.19.0",
        "@taiga-ui/styles": "^3.19.0",
        "@tinkoff/ng-dompurify": "4.0.0",
        "@tinkoff/ng-polymorpheus": "^4.0.10",
        "dompurify": "3.0.0",
        "rxjs": "~7.8.0",
        "tslib": "^2.5.0",
        "uuid": "^9.0.0",
        "zone.js": "~0.12.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~15.2.0",
        "@angular/cli": "~15.2.0",
        "@angular/compiler-cli": "~15.2.0",
        "@types/dompurify": "2.4.0",
        "@types/jasmine": "~4.3.1",
        "@types/node": "^18.14.0",
        "@types/uuid": "^9.0.1",
        "angular-cli-ghpages": "^1.0.5",
        "colors": "^1.4.0",
        "dotenv": "^16.0.3",
        "jasmine-core": "~4.5.0",
        "karma": "~6.4.1",
        "karma-chrome-launcher": "~3.1.1",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.0.0",
        "prettier": "^2.8.4",
        "ts-mockito": "^2.6.1",
        "ts-node": "^10.9.1",
        "typescript": "~4.9.5",
        "webpack-bundle-analyzer": "^4.4.2"
    }
}
